{
  "name": "brain",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^4",
    "@sap/xb-msg-amqp-v100": "latest",
    "console-log-level": "^1.4.1",
    "express": "^4"
  },
  "devDependencies": {
    "sqlite3": "^4"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "messaging": {
        "kind": "enterprise-messaging"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "brain.db"
        }
      },
      "S4SalesOrders": {
        "kind": "odata",
        "model": "srv/external/API_SALES_ORDER_SRV",
        "credentials": {
          "destination": "apihub_mock_salesorders",
          "path": "/sap/opu/odata/sap/API_SALES_ORDER_SRV"
        }
      },
      "conversionservice": {
        "kind": "rest",
        "model": "srv/cds/ext-service",
        "credentials": {
          "url": "https://mock-converter-chatty-gorilla-ve.cfapps.eu10.hana.ondemand.com/conversion"
        }
      }
    }
  }
}
