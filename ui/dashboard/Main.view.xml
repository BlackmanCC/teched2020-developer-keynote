<mvc:View xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.f"
	xmlns:w="sap.ui.integration.widgets">
	<ShellBar title="Developer Keynote" secondTitle="Analytics" homeIcon="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg.adapt.svg/1493030643828.svg"/>

	<GridContainer>
		<html:style>
      .sapFGridContainer {
         margin-top: 2rem;
      }
		</html:style>
		<layout>
			<GridContainerSettings rowSize="5rem" columnSize="5rem" gap="1rem" />
		</layout>
		<layoutS>
			<GridContainerSettings rowSize="5rem" columnSize="5rem" gap="0.5rem" />
		</layoutS>
		<w:Card manifest="{cards>/stackedColumnCard}" baseUrl=".">
			<w:layoutData>
				<GridContainerItemLayoutData rows="5" columns="4" />
			</w:layoutData>
		</w:Card>
		<w:Card manifest="{cards>/linesCard}" baseUrl=".">
			<w:layoutData>
				<GridContainerItemLayoutData rows="5" columns="4" />
			</w:layoutData>
		</w:Card>
		<w:Card manifest="{cards>/donutCard}" baseUrl=".">
			<w:layoutData>
				<GridContainerItemLayoutData rows="5" columns="4" />
			</w:layoutData>
		</w:Card>
	</GridContainer>
</mvc:View>
