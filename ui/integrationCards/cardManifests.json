	"donutCard": {
		"sap.app": {
			"id": "sap.developerkeynote.donutCard",
			"type": "card"
		},
		"sap.card": {
			"type": "Analytical",
			"extension": "./extension",
			"data": {
				"extension": {
					"method": "getDerivedData"
				}
			},
			"header": {
				"type": "Numeric",
				"title": "Credits Breakdown",
				"mainIndicator": {
					"number": "{customers/length}",
					"unit": "Customers"
				},
				"sideIndicators": [
