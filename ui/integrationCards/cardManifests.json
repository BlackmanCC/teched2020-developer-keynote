					},
					{
						"title": "Total",
						"number": "{total}"
					}
				]
			},
			"content": {
				"chartType": "Donut",
				"legend": {
					"visible": true,
					"position": "Bottom",
					"alignment": "Center"
				},
				"plotArea": {
					"dataLabel": {
						"visible": true,
						"showTotal": true
					}
				},
				"title": {
					"visible": true,
					"text": "Credits per Customers",
					"alignment": "Left"
				},
				"measureAxis": "size",
				"dimensionAxis": "color",
				"data": {
					"path": "/customers"
				},
				"dimensions": [
					{
						"label": "Customer",
						"value": "Customer {customer}"
					}
				],
				"measures": [
					{
						"label": "Credits",
						"value": "{total}"
					}
				]
			}
		}
