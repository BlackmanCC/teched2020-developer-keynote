{
	"stackedColumnCard": {
		"sap.app": {
			"id": "sap.developerkeynote.stacksCard",
			"type": "card"
		},
		"sap.card": {
			"type": "Analytical",
			"extension": "./extension",
			"data": {
				"extension": {
					"method": "getDerivedData"
				}
			},
			"header": {
				"icon": {},
				"type": "Numeric",
				"title": "Collected Credits - Q4, 2020",
				"mainIndicator": {
					"number": "{growth}",
					"unit": "%",
					"trend": "Up",
					"state": "Good"
				}
			},
			"content": {
				"chartType": "StackedColumn",
				"data": {
					"path": "/months"
				},
				"legend": {
					"visible": true,
					"position": "Bottom",
					"alignment": "Center"
				},
				"plotArea": {
					"dataLabel": {
						"visible": false,
						"showTotal": true
					},
					"categoryAxisText": {
						"visible": false
					},
					"valueAxisText": {
						"visible": false
					}
				},
				"title": {
					"visible": false
				},
				"measureAxis": "valueAxis",
				"dimensionAxis": "categoryAxis",
				"dimensions": [
					{
						"label": "Month",
						"value": "{= format.dateTime(${date}, {format: 'yMMM'})}"
					}
				],
				"measures": [
					{
