{
	"stackedColumnCard": {
		"sap.app": {
			"id": "sap.developerkeynote.stacksCard",
			"type": "card"
		},
		"sap.card": {
			"type": "Analytical",
			"extension": "./extension",
			"data": {
				"extension": {
					"method": "getDerivedData"
				}
			},
			"header": {
				"icon": {},
				"type": "Numeric",
				"title": "Accumulated Credits",
				"mainIndicator": {
					"number": "{achievement}",
					"unit": "%",
					"trend": "Up",
					"state": "Good"
				},
				"details": "Over target",
				"sideIndicators": [
					{
						"title": "Target",
						"number": "{target}"
					},
					{
						"title": "Total",
						"number": "{total}"
					}
				]
			},
			"content": {
				"chartType": "StackedColumn",
				"data": {
					"path": "/months"
				},
				"legend": {
					"visible": true,
					"position": "Bottom",
					"alignment": "Center"
				},
				"plotArea": {
					"dataLabel": {
						"visible": false,
						"showTotal": true
					},
					"categoryAxisText": {
						"visible": false
					},
					"valueAxisText": {
						"visible": false
					}
				},
				"title": {
					"visible": false
				},
				"measureAxis": "valueAxis",
				"dimensionAxis": "categoryAxis",
				"dimensions": [
					{
						"label": "Month",
						"value": "{= format.dateTime(${date}, {format: 'yMMM'})}"
					}
				],
				"measures": [
					{
						"value": "{USCU-CUS01}",
						"label": "World of Sports Inc."
					},
					{
						"value": "{USCU-CUS02}",
						"label": "Roadkings Sports Inc."
					},
					{
						"value": "{USCU-CUS04}",
						"label": "Unbeatable Sports"
					}
				]
			}
		}
	},
	"linesCard": {
		"sap.app": {
			"id": "sap.developerkeynote.linesCard",
			"type": "card"
		},
		"sap.card": {
			"type": "Analytical",
			"extension": "./extension",
			"data": {
				"extension": {
					"method": "getDerivedData"
				}
			},
			"header": {
				"type": "Numeric",
				"title": "Collected Credits",
				"mainIndicator": {
					"number": "{avgGrowth}",
					"unit": "%",
					"trend": "Up",
					"state": "Good"
				},
				"details": "Avergage Growth",
				"sideIndicators": [
					{
						"title": "Smallest Growth",
						"number": "{smallestG} %"
					},
					{
						"title": "Largest Growth",
						"number": "{largestG} %"
					}
				]
			},
			"content": {
				"chartType": "Line",
				"data": {
					"path": "/months"
				},
				"legend": {
					"visible": true,
					"position": "Bottom",
					"alignment": "Center"
				},
				"plotArea": {
					"dataLabel": {
						"visible": false,
						"showTotal": true
					},
					"categoryAxisText": {
						"visible": false
					},
					"valueAxisText": {
						"visible": false
					}
				},
				"title": {
					"visible": false
				},
				"measureAxis": "valueAxis",
				"dimensionAxis": "categoryAxis",
				"dimensions": [
					{
						"label": "Month",
						"value": "{= format.dateTime(${date}, {format: 'yMMM'})}"
					}
				],
				"measures": [
					{
						"value": "{USCU-CUS01}",
						"label": "World of Sports Inc."
					},
					{
						"value": "{USCU-CUS02}",
						"label": "Roadkings Sports Inc."
					},
					{
						"value": "{USCU-CUS04}",
						"label": "Unbeatable Sports"
					}
				]
			}
		}
	},
	"donutCard": {
		"sap.app": {
			"id": "sap.developerkeynote.donutCard",
			"type": "card"
		},
		"sap.card": {
			"type": "Analytical",
			"extension": "./extension",
			"data": {
				"extension": {
					"method": "getDerivedData"
				}
			},
			"header": {
				"type": "Numeric",
				"title": "Credits Breakdown",
				"mainIndicator": {
					"number": "{customers/length}",
					"unit": "Customers"
				},
				"details": "Q4, 2020"
			},
			"content": {
				"chartType": "Donut",
				"legend": {
					"visible": true,
					"position": "Bottom",
					"alignment": "Center"
				},
				"plotArea": {
					"dataLabel": {
						"visible": true,
						"showTotal": true
					}
				},
				"title": {
					"visible": false
				},
				"measureAxis": "size",
				"dimensionAxis": "color",
				"data": {
					"path": "/customers"
				},
				"dimensions": [
					{
						"label": "Customer",
						"value": "{customername}"
					}
				],
				"measures": [
					{
						"label": "Credits",
						"value": "{total}"
					}
				]
			}
		}
	}
}